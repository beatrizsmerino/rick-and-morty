{
  "source": {
    "include": [
      "assets/js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "exclude": [
      "node_modules"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./jsdocs/",
    "recurse": true,
    "readme": "./README.md"
  }
}